{
    "name": "brunofalcaodev/nidavellir-cube",
    "description": "Nidavellir Cube",
    "keywords": [
        "brunofalcaodev"
    ],
    "homepage": "https://github.com/brunofalcaodev/nidavellir-cube",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bruno Falc√£o",
            "email": "me@brunofalcao.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0",
        "brunofalcaodev/nidavellir-abstracts": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nidavellir\\Cube\\": "src/",
            "Nidavellir\\Database\\Factories\\": "database/factories/",
            "Nidavellir\\Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nidavellir\\Cube\\NidavellirCubeServiceProvider"
            ]
        }
    }
}
